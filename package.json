{
  "name": "stlog-notification-service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deploy": "NODE_ENV=production && tsc && sls deploy --stage prod",
    "deploy-dev": "NODE_ENV=production && tsc && sls deploy --stage dev",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "debug": "NODE_ENV=dev && ts-node src/bin/www.ts",
    "offline": "NODE_ENV=dev && tsc && cp -R src/views dist/ && sls offline",
    "start": "NODE_ENV=production && tsc && node dist/bin/www.js",
    "tsc": "tsc && cp -R src/views dist/",
    "test-coverage": "jest --coverage",
    "test": "jest --forceExit",
    "test-watch": "jest --watch",
    "test-verbose": "jest --verbose"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      ".deployment/",
      "/backend-chart/",
      "/test/",
      "utils/",
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@sendgrid/mail": "^7.2.6",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.8",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.11.2",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.0",
    "cookie-parser": "~1.4.5",
    "debug": "~4.2.0",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "express-healthcheck": "^0.1.0",
    "express-validator": "^6.6.1",
    "helmet": "^4.1.1",
    "http-errors": "~1.8.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.10.0",
    "pug": "^3.0.0",
    "serverless": "^2.2.0",
    "serverless-domain-manager": "^5.0.0",
    "serverless-http": "^2.6.0",
    "twilio": "^3.49.3",
    "typescript": "^4.0.3",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "acorn": "^8.0.1",
    "aws-sdk": "^2.760.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "serverless-offline": "^6.8.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0"
  }
}
