{
  "name": "stlog-notification-service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deploy": "NODE_ENV=production && tsc && sls deploy",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "debug": "NODE_ENV=dev && ts-node src/bin/www.ts",
    "offline": "NODE_ENV=dev && tsc && cp -R src/views dist/ && sls offline",
    "start": "NODE_ENV=production && tsc && node dist/bin/www.js",
    "tsc": "tsc && cp -R src/views dist/",
    "test-coverage": "jest --coverage",
    "test": "jest --forceExit",
    "test-watch": "jest --watch",
    "test-verbose": "jest --verbose"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      ".deployment/",
      "/backend-chart/",
      "/test/",
      "utils/",
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@sendgrid/mail": "^7.2.3",
    "@types/bcrypt": "^3.0.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.7",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.27",
    "@types/uuid": "^8.3.0",
    "aws-sdk": "^2.731.0",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "cookie-parser": "~1.4.5",
    "debug": "~4.1.1",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "express-healthcheck": "^0.1.0",
    "helmet": "^4.0.0",
    "http-errors": "~1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.28",
    "morgan": "~1.10.0",
    "multer": "^1.4.2",
    "pug": "^3.0.0",
    "serverless": "^1.78.1",
    "serverless-http": "^2.5.0",
    "ssh2-sftp-client": "^5.2.2",
    "twilio": "^3.49.1",
    "typescript": "^3.9.7",
    "update-notifier": "^4.1.0",
    "uuid": "^8.3.0",
    "validator": "^13.1.1",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "acorn": "^7.4.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.3.0",
    "prettier": "^2.0.5",
    "serverless-offline": "^6.7.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2"
  }
}
