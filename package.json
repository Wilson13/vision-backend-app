{
  "name": "vision-backend-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deploy": "NODE_ENV=production && tsc && sls deploy --stage prod",
    "deploy-dev": "NODE_ENV=production && tsc && sls deploy --stage dev",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "debug": "NODE_ENV=dev && ts-node src/bin/www.ts",
    "offline": "NODE_ENV=dev && tsc && cp -R src/views dist/ && sls offline",
    "start": "NODE_ENV=production && tsc && node dist/bin/www.js",
    "tsc": "tsc && cp -R src/views dist/",
    "test-coverage": "jest --coverage",
    "test": "jest --forceExit",
    "test-watch": "jest --watch",
    "test-verbose": "jest --verbose"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      ".deployment/",
      "/backend-chart/",
      "/test/",
      "utils/",
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@google-cloud/vision": "^2.3.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.20",
    "@types/mongoose": "^5.10.3",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.25",
    "@types/sharp": "^0.27.1",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "cookie-parser": "~1.4.5",
    "debug": "~4.3.1",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "express-healthcheck": "^0.1.0",
    "express-validator": "^6.9.2",
    "helmet": "^4.4.1",
    "http-errors": "~1.8.0",
    "image-size": "^0.9.3",
    "morgan": "~1.10.0",
    "multer": "^1.4.2",
    "pug": "^3.0.0",
    "serverless": "^2.22.0",
    "serverless-domain-manager": "^5.1.0",
    "serverless-http": "^2.7.0",
    "typescript": "^4.1.3",
    "validator": "^13.5.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "acorn": "^8.0.5",
    "aws-sdk": "^2.838.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "serverless-offline": "^6.8.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1"
  }
}
